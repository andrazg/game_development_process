@inproceedings{greuter_game_2014,
	location = {New York, {NY}, {USA}},
	title = {Game Asset Repetition},
	isbn = {978-1-4503-2790-9},
	url = {http://doi.acm.org/10.1145/2677758.2677782},
	doi = {10.1145/2677758.2677782},
	series = {{IE}2014},
	abstract = {The frequent repetition of visual assets, such as the frequent appearance of a particular game object in a game world or the repetition of a character's animation cycle, often becomes apparent to players when they encounter such repetition within a short period of time. A certain amount of visual repetition has always been accepted by players, however as technology improves and game worlds tend towards more detail, the repetition of assets in games becomes more obvious. Particularly graphically advanced games require an increasing number of assets to hide the repetition and to create believable game worlds. This paper examines various levels of asset repetition in electronic games and addresses problems that can arise. The paper describes some contemporary approaches used by artists in the industry to hide repetition, and touches on current technologies that might be applied in game development to address this problem.},
	pages = {20:1--20:5},
	booktitle = {Proceedings of the 2014 Conference on Interactive Entertainment},
	publisher = {{ACM}},
	author = {Greuter, Stefan and Nash, Adam},
	urldate = {2018-01-09},
	date = {2014},
	keywords = {Asset Repetition, Game Assets, Procedural Generation}
}

@inproceedings{zagal_placeholder_2015,
	location = {New York, {NY}, {USA}},
	title = {Placeholder Content in Game Development: Benefits and Challenges},
	isbn = {978-1-4503-3466-2},
	url = {http://doi.acm.org/10.1145/2793107.2810319},
	doi = {10.1145/2793107.2810319},
	series = {{CHI} {PLAY} '15},
	shorttitle = {Placeholder Content in Game Development},
	abstract = {Literature on game design and development recommends using placeholder content and assets as a technique for, amongst other things, working around production bottlenecks (e.g. needs music to work but music is not ready) and streamlining development (e.g. focus on the "fun", without getting distracted by the art). However, this commonly accepted practice can cause problems that are rarely discussed or presented. We unpack this practice by discussing some of its issues. As a practice that is central to game development, it is important to better understand the interactions of social, technical, and cultural factors that result in the creation of sophisticated software artifacts, i.e. computer games. We discuss some of these issues and sketch out a research agenda for their further exploration.},
	pages = {745--750},
	booktitle = {Proceedings of the 2015 Annual Symposium on Computer-Human Interaction in Play},
	publisher = {{ACM}},
	author = {Zagal, José P. and Altizer, Roger},
	urldate = {2018-01-09},
	date = {2015},
	keywords = {video games, game development, dummy text, game design, lorem ipsum, placeholder, placeholder assets, temporary assets, white box}
}

